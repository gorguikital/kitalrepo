<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Utilisateur.
    -->
    <changeSet id="20220701163842-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="service_id"
                                 baseTableName="utilisateurs"
                                 constraintName="fk_utilisateurs_service_id"
                                 referencedColumnNames="id"
                                 referencedTableName="services"/>

        <addForeignKeyConstraint baseColumnNames="utilisateur_id"
                                 baseTableName="utilisateurs_profile"
                                 constraintName="fk_utilisateurs_profile_utilisateurs_id"
                                 referencedColumnNames="id"
                                 referencedTableName="utilisateurs"/>

        <addForeignKeyConstraint baseColumnNames="profile_id"
                                 baseTableName="utilisateurs_profile"
                                 constraintName="fk_utilisateurs_profile_profile_id"
                                 referencedColumnNames="id"
                                 referencedTableName="profiles"/>
        
    </changeSet>
</databaseChangeLog>
